/* Komplexnejsi test pre pracu s retazcami
*/

class Main
{
	static void run()
	{
		String prvy = "Toto je dlhy text ";
		String druhy = prvy + ", este dlhsi retazec";

		String vys = prvy + druhy + " koniec.";

		int vysledok = vyhladaj(vys);

		// Vypis retazec a kde si to nasiel
		ifj16.print(vys + "\n" + vysledok);
	}

	static int vyhladaj(String vstup)
	{
		int result = ifj16.find(vstup, "text");
		return result;
	}
}
