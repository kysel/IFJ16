// Testovanie if a while

class Main
{
	static void run()
	{
		double d = func();
		ifj16.print("Vysledok: " + d + "\n");
	}

	static double func()
	{
		int a = 10;
		double b = 2.5;

		while (a > 5)
		{
			ifj16.print("Spravne a" + "\n");

			if (b != 2.5)
			{
				ifj16.print("Tu nemam byt" + "\n");
				return 0.5;
			}
			else
			{
				while (b > 0)
				{
					ifj16.print("Spravne b: " + b + "\n");
					b = b - 0.5;
				}

				return 2.58;
			}
		}

		return 2.58;
	}
}