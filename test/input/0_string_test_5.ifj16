// Celkove testovanie ifj16 funkcii

class Main
{
	static void run()
	{
		String sub = ifj16.substr("ahoj", 1, 3);
		ifj16.print(sub + "\n");

		int vyskyt = ifj16.find("ahoj", sub);
		ifj16.print(vyskyt + "\n");

		int len = ifj16.length(sub);
		ifj16.print(len + "\n");

		String srt = ifj16.sort(sub);
		ifj16.print(srt + "\n");

		int cmp = ifj16.compare("hjo", srt);
		ifj16.print(cmp + "\n");
	}
}